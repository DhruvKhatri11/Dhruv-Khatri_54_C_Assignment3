#include <stdio.h>
int main() {
    int a[10], n, pos, i;
    printf("Enter size: ");
    scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter position to delete: ");
    scanf("%d",&pos);
    for(i=pos;i<n-1;i++) a[i]=a[i+1];
    n--;
    for(i=0;i<n;i++) printf("%d ",a[i]);
    return 0;
}

Output: Enter size: 5
10 20 30 40 50
Enter position to delete: 2
10 20 40 50 
