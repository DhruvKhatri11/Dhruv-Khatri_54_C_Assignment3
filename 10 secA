#include <stdio.h>
int main() {
    int a[10], freq[100]={0}, i;
    printf("Enter 10 numbers: ");
    for(i=0;i<10;i++) {scanf("%d",&a[i]); freq[a[i]]++;}
    printf("Frequencies:\n");
    for(i=0;i<100;i++) if(freq[i]>0) printf("%d -> %d times\n",i,freq[i]);
    return 0;
}

Output: Enter 10 numbers: 1 5 3 5 7 1 9 3 5 1
Frequencies: 
1 - 3 times
3 - 2 times
5 - 3 times
7 - 1 time
9 - 1 time


