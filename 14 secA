#include <stdio.h>
int main() {
    int a[10], n, val, i;
    printf("Enter size: ");
    scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter value: ");
    scanf("%d",&val);
    a[n]=val; n++;
    for(i=0;i<n;i++) printf("%d ",a[i]);
    return 0;
}

Output: Enter size: 4
1 2 3 4
Enter value: 9
1 2 3 4 9
