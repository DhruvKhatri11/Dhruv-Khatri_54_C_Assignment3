#include <stdio.h>
int main() {
    int a[10], n, i, x;
    printf("Enter size: ");
    scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Enter number to insert: ");
    scanf("%d",&x);
    for(i=n;i>0;i--) a[i]=a[i-1];
    a[0]=x; n++;
    printf("Array: ");
    for(i=0;i<n;i++) printf("%d ",a[i]);
    return 0;
}

Output: Enter size : 4
1 2 3 4
Enter number to insert : 9
9 1 2 3 4
